local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "Key System", HidePremium = false, SaveConfig = true, IntroEnabled = false})

OrionLib:MakeNotification({
	Name = "Logged in!",
	Content = "You are logged in as "..Player.Name..". ",
	Image = "rbxassetid://4483345998",
	Time = 5
})

_G.Key = "DAORA123"
_G.KeyInput = "string"

function MakeScriptHub()
if game.PlaceId == 14236123211 then

    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Tutuz HUB", HidePremium = false, SaveConfig = false, ConfigFolder = "testetutuz"})
local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Automatic"
})
Tab:AddButton({
	Name = "Auto Punch (on fight)",
	Callback = function()
		while true do wait() if game:GetService("Players").LocalPlayer.PlayerGui.FightingMainUi.Enabled == true then game:GetService("VirtualInputManager"):SendMouseButtonEvent(1, 1, 0, true, game, 0) end end
  	end    
})
local autotrain = false
Tab:AddToggle({
	Name = "Auto Train",
	Default = false,
	Callback = function(Value)
		autotrain = Value
	end    
})
while true do wait() if autotrain == true then game:GetService("ReplicatedStorage").Events.DamageIncreaseOnClickEvent:FireServer() end end

end 

local TPTab = Window:MakeTab({
	Name = "Teleport",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Teleports"
})
function CorrectKeyNotification()
OrionLib:MakeNotification({
	Name = "Correct Key!",
	Content = "You have entered the correct key!",
	Image = "rbxassetid://4483345998",
	Time = 5
})
end
end 

function IncorrectKeyNotification()
OrionLib:MakeNotification({
	Name = "Incorrect Key!",
	Content = "You have entered the incorrect key!",
	Image = "rbxassetid://4483345998",
	Time = 5
})
end


local Tab = Window:MakeTab({
	Name = "Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Key",
	Default = " ",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "Check Key",
	Callback = function()
      	if _G.KeyInput == _G.Key then
        MakeScriptHub()
        CorrectKeyNotification()
        else
            IncorrectKeyNotification()
        end
  	end    
})

